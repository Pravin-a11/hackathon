int searchInsert(int* nums, int numsSize, int target) 
{
    int l=0;
    int n=numsSize-1;
    while(l<=n)
    {
        int m=(l+n)/2;
        if(nums[m]<target)
        {
            l=m++;
        }
        else if(nums[m]>target)
        {
            n=m--;
        }
        else
        {
            return m;
        }
    }
    return l;  
}
